{
  "name": "@morphql/cli",
  "version": "0.1.25",
  "description": "CLI tool for morphql - transform structural data from the command line.",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "morphql": "./bin/morphql.js"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --clean",
    "build:qjs": "esbuild src/qjs.ts --bundle --format=esm --outfile=dist/qjs/qjs.js --external:stream --external:fs --external:path --external:node:fs --external:node:path --external:node:stream --platform=browser --main-fields=module,main --minify",
    "dev": "tsup src/index.ts --format esm --watch",
    "test": "node --experimental-vm-modules node_modules/.bin/vitest run",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "morphql",
    "cli",
    "transform",
    "json",
    "xml",
    "edifact",
    "edi"
  ],
  "author": "Hyperwindmill",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hyperwindmill/morphql.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/Hyperwindmill/morphql/issues"
  },
  "homepage": "https://github.com/Hyperwindmill/morphql/tree/main/packages/cli#readme",
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@morphql/core": "*",
    "commander": "^14.0.3"
  },
  "devDependencies": {
    "@types/node": "^25.2.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^3.0.0"
  }
}
